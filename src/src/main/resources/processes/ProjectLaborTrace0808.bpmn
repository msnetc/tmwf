  <process id="ProjectLaborTrace0808" name="项目人力跟踪流程" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <userTask id="pmTask" name="项目经理审核" activiti:assignee="${pmUser}">
      <extensionElements>
        <activiti:taskListener event="complete" class="com.taimeitech.pass.workflowExt.Listener.finance.ProjectLaborTrace.PmTaskLIstener"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="pmUsers" activiti:elementVariable="pmUser"></multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="financeTask" name="财务人员审核" activiti:assignee="${financeUser}">
      <extensionElements>
        <activiti:taskListener event="complete" class="com.taimeitech.pass.workflowExt.Listener.finance.ProjectLaborTrace.FinanceTaskListener"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="financeUsers" activiti:elementVariable="financeUser"></multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="submitcomfire" name="用户确认" activiti:assignee="${createUser}"></userTask>
    <userTask id="submitupdate" name="用户修改" activiti:assignee="${createUser}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="pmTask">
      <extensionElements>
        <activiti:executionListener event="take" expression="${execution.setVariable('approvedPmCounter', 0)}"></activiti:executionListener>
        <activiti:executionListener event="take" expression="${execution.setVariable('approvedFinanceUsersCounter', 0)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow2" name="pm审核通过" sourceRef="pmTask" targetRef="financeTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedPmCounter == pmUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow3" name="PM主管审核通过" sourceRef="financeTask" targetRef="submitcomfire">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedFinanceUsersCounter == financeUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" name="pm审核驳回" sourceRef="pmTask" targetRef="submitupdate">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedPmCounter < pmUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="财务人员驳回" sourceRef="financeTask" targetRef="submitupdate">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedFinanceUsersCounter < financeUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" name="确认失败" sourceRef="submitcomfire" targetRef="submitupdate">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${confirmed==false}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow7" name="确认成功" sourceRef="submitcomfire" targetRef="endevent1">
       <extensionElements>
        <activiti:executionListener event="take" class="com.taimeitech.pass.workflowExt.Listener.TaskEndListener"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${confirmed==true}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="submitupdate" targetRef="pmTask"></sequenceFlow>
  </process>
