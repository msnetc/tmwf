  <process id="LaborHourTrace0808test" name="工时表跟踪" isExecutable="true">
    <startEvent id="startevent1" name="Start"></startEvent>
    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="usertask1" name="主审审核" activiti:assignee="${pmUser}">
      <extensionElements>
        <activiti:taskListener event="complete" class="com.taimeitech.pass.workflowExt.Listener.CommonCountListener"></activiti:taskListener>
      </extensionElements>
      <multiInstanceLoopCharacteristics isSequential="false" activiti:collection="pmUsers" activiti:elementVariable="pmUser"></multiInstanceLoopCharacteristics>
    </userTask>
    <userTask id="usertask2" name="用户修改" activiti:assignee="${createUser}"></userTask>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow1" name="提交" sourceRef="startevent1" targetRef="usertask1">
      <extensionElements>
        <activiti:executionListener event="take" expression="${execution.setVariable('totalApprovedCount', 0)}"></activiti:executionListener>
      </extensionElements>
    </sequenceFlow>
    <sequenceFlow id="flow3" name="审核拒绝" sourceRef="exclusivegateway1" targetRef="usertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${totalApprovedCount < pmUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow4" name="审核通过" sourceRef="exclusivegateway1" targetRef="endevent1">
      <extensionElements>
        <activiti:executionListener event="take" class="com.taimeitech.pass.workflowExt.Listener.TaskEndListener"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${totalApprovedCount == pmUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" sourceRef="usertask2" targetRef="usertask1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="usertask1" targetRef="exclusivegateway1">
    </sequenceFlow>
  </process>
