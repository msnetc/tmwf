      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedFinanceUsersCounter == financeUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent1" name="End"></endEvent>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow9" name="财务主管驳回" sourceRef="financeTask" targetRef="endevent2">
      <extensionElements>
        <activiti:executionListener event="take" class="com.taimeitech.pass.workflowExt.Listener.TaskRejectionListener"></activiti:executionListener>
      </extensionElements>
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${approvedFinanceUsersCounter < financeUsers.size()}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" name="pm主管驳回" sourceRef="pmTask" targetRef="endevent2">
